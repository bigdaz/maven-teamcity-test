<gradleEnterprise>
  <buildScan>
        <termsOfService>
          <url>https://gradle.com/terms-of-service</url>
          <accept>true</accept>
        </termsOfService>
        <captureGoalInputFiles>true</captureGoalInputFiles>
    </buildScan>
    <buildCache>
        <local>
            <enabled>false</enabled>
        </local>
        <remote>
            <enabled>false</enabled>
        </remote>
    </buildCache>
</gradleEnterprise>
